{
  "npmScope": "cool-magazine",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "identity-node-oidc-provider": {
      "tags": ["node-oidc-provider"]
    },
    "identity-oidc-core": {
      "tags": ["oidc-core"]
    },
    "services-identity-service": {
      "tags": ["identity-service"]
    },
    "shared-ddd-expc": {
      "tags": ["ddd-expc"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
