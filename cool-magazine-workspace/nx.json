{
  "npmScope": "cool-magazine",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "adapter-identity-node-oidc-provider": {
      "tags": ["adapter", "identity", "node-oidc-provider"]
    },
    "domains-generic-identity-oidc-admin-site": {
      "tags": ["generic", "subdomain", "identity", "oidc-admin-site"]
    },
    "generic-subdomain-identity-oidc-core": {
      "tags": ["generic", "subdomain", "identity", "oidc-core"]
    },
    "services-identity-service": {
      "tags": ["identity-service"]
    },
    "shared-angular-cdk": {
      "tags": ["shared", "angular", "cdk", "angular-cdk"]
    },
    "shared-ddd-expc": {
      "tags": ["ddd-expc"]
    },
    "shared-styles": {
      "tags": []
    },
    "web-oidc-admin": {
      "tags": ["apps", "web", "oidc-admin"],
      "implicitDependencies": ["shared-styles"]
    },
    "web-oidc-admin-e2e": {
      "tags": [],
      "implicitDependencies": ["web-oidc-admin"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
