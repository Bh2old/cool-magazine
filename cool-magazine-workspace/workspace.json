{
  "version": 2,
  "projects": {
    "identity-node-oidc-provider": {
      "root": "libs/identity/node-oidc-provider",
      "sourceRoot": "libs/identity/node-oidc-provider/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/identity/node-oidc-provider",
            "main": "libs/identity/node-oidc-provider/src/index.ts",
            "tsConfig": "libs/identity/node-oidc-provider/tsconfig.lib.json",
            "assets": ["libs/identity/node-oidc-provider/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/identity/node-oidc-provider/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/identity/node-oidc-provider"],
          "options": {
            "jestConfig": "libs/identity/node-oidc-provider/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "services-identity-service": {
      "root": "apps/services/identity-service",
      "sourceRoot": "apps/services/identity-service/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/services/identity-service",
            "main": "apps/services/identity-service/src/main.ts",
            "tsConfig": "apps/services/identity-service/tsconfig.app.json",
            "assets": ["apps/services/identity-service/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/services/identity-service/src/environments/environment.ts",
                  "with": "apps/services/identity-service/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "services-identity-service:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/services/identity-service/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/services/identity-service"],
          "options": {
            "jestConfig": "apps/services/identity-service/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "services-identity-service"
}
