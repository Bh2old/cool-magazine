{
  "version": 2,
  "projects": {
    "adapter-identity-node-oidc-provider": {
      "root": ".",
      "sourceRoot": "libs/adapters/identity/node-oidc-provider/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/adapters/identity/node-oidc-provider",
            "main": "libs/adapters/identity/node-oidc-provider/src/index.ts",
            "tsConfig": "libs/adapters/identity/node-oidc-provider/tsconfig.lib.json",
            "assets": ["libs/adapters/identity/node-oidc-provider/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/adapters/identity/node-oidc-provider/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/adapters/identity/node-oidc-provider"],
          "options": {
            "jestConfig": "libs/adapters/identity/node-oidc-provider/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "domains-generic-identity-oidc-admin-site": {
      "root": "libs/domains/generic/identity/oidc-admin-site",
      "sourceRoot": "libs/domains/generic/identity/oidc-admin-site/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/domains/generic/identity/oidc-admin-site",
            "main": "libs/domains/generic/identity/oidc-admin-site/src/index.ts",
            "tsConfig": "libs/domains/generic/identity/oidc-admin-site/tsconfig.lib.json",
            "assets": ["libs/domains/generic/identity/oidc-admin-site/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/domains/generic/identity/oidc-admin-site/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/domains/generic/identity/oidc-admin-site"],
          "options": {
            "jestConfig": "libs/domains/generic/identity/oidc-admin-site/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "generic-subdomain-identity-oidc-core": {
      "root": ".",
      "sourceRoot": "libs/domains/generic/identity/oidc-core/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/domains/generic/identity/oidc-core",
            "main": "libs/domains/generic/identity/oidc-core/src/index.ts",
            "tsConfig": "libs/domains/generic/identity/oidc-core/tsconfig.lib.json",
            "assets": ["libs/domains/generic/identity/oidc-core/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/domains/generic/identity/oidc-core/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/domains/generic/identity/oidc-core"],
          "options": {
            "jestConfig": "libs/domains/generic/identity/oidc-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "services-identity-service": {
      "root": ".",
      "sourceRoot": "apps/services/identity-service/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/services/identity-service",
            "main": "apps/services/identity-service/src/main.ts",
            "tsConfig": "apps/services/identity-service/tsconfig.app.json",
            "assets": ["apps/services/identity-service/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/services/identity-service/src/environments/environment.ts",
                  "with": "apps/services/identity-service/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "services-identity-service:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/services/identity-service/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/services/identity-service"],
          "options": {
            "jestConfig": "apps/services/identity-service/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ddd-expc": {
      "root": ".",
      "sourceRoot": "libs/shared/ddd-expc/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/shared/ddd-expc",
            "main": "libs/shared/ddd-expc/src/index.ts",
            "tsConfig": "libs/shared/ddd-expc/tsconfig.lib.json",
            "assets": ["libs/shared/ddd-expc/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/ddd-expc/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ddd-expc"],
          "options": {
            "jestConfig": "libs/shared/ddd-expc/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "web-oidc-admin": {
      "projectType": "application",
      "root": "apps/web/oidc-admin",
      "sourceRoot": "apps/web/oidc-admin/src",
      "prefix": "app",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/web/oidc-admin",
            "index": "apps/web/oidc-admin/src/index.html",
            "main": "apps/web/oidc-admin/src/main.ts",
            "polyfills": "apps/web/oidc-admin/src/polyfills.ts",
            "tsConfig": "apps/web/oidc-admin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/web/oidc-admin/src/favicon.ico",
              "apps/web/oidc-admin/src/assets"
            ],
            "styles": ["apps/web/oidc-admin/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/web/oidc-admin/src/environments/environment.ts",
                  "with": "apps/web/oidc-admin/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "web-oidc-admin:build:production"
            },
            "development": {
              "browserTarget": "web-oidc-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-oidc-admin:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/web/oidc-admin/src/**/*.ts",
              "apps/web/oidc-admin/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/web/oidc-admin"],
          "options": {
            "jestConfig": "apps/web/oidc-admin/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "web-oidc-admin-e2e": {
      "root": "apps/web/oidc-admin-e2e",
      "sourceRoot": "apps/web/oidc-admin-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/web/oidc-admin-e2e/cypress.json",
            "devServerTarget": "web-oidc-admin:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "web-oidc-admin:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/web/oidc-admin-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "services-identity-service",
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
